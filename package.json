{
  "name": "otodo",
  "version": "0.0.1",
  "description": "A Static site for oToDo Le hub",
  "main": "dist/index.html",
  "scripts": {
    "lint:css": "stylelint 'src/**/*.css'",
    "lint": "run-p lint:*",
    "clean": "rimraf dist/{css/*,js/*,img/*}",
    "prebuild": "npm run clean",
    "build:css": "postcss -c .postcssrc.json",
    "build:js": "babel src/ --out-dir dist/js",
    "build:html": "nunjucks *.njk -p src -o dist",
    "build": "run-p build:*",
    "watch:css": "postcss -c .postcssrc.json -w --poll",
    "watch:js": "babel src/ --watch --out-dir dist/js --source-maps inline",
    "watch:nunjucks": "nunjucks -w *.njk -p src -o dist",
    "watch": "run-p watch:*",
    "wiredep": "wiredep -s src/app.css",
    "serve": "browser-sync start --config '.browsersyncrc.json'",
    "start": "run-p watch serve",
    "minify:css": "cssnano dist/main.css dist/main.css --safe --no-autoprexifer",
    "minify:js": "uglifyjs dist/js/app.js --compress --mangle --output dist/js/app.js",
    "minify": "npm-run-all --parallel minify:*"
  },
  "author": "Sally Northmore",
  "license": "ISC",
  "repository": {},
  "contributors": [
    {
      "name": "Sally Northmore",
      "email": "sally.northmore@gmail.com",
      "url": "https://github.com/sallynorthmore"
    }
  ],
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.14.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.14.0",
    "chokidar-cli": "^1.2.0",
    "copy": "^0.3.0",
    "cssnano-cli": "^1.0.4",
    "install": "^0.8.1",
    "npm": "^3.10.8",
    "npm-run-all": "^3.0.0",
    "nunjucks": "^3.0.0",
    "nunjucks-cli": "^0.4.9",
    "postcss-calc": "^5.3.1",
    "postcss-cli": "^2.5.2",
    "postcss-color-function": "^2.0.1",
    "postcss-color-gray": "^3.0.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-import": "^8.1.2",
    "postcss-inline-svg": "^2.1.1",
    "postcss-nesting": "^2.3.1",
    "rimraf": "^2.5.4",
    "stylelint": "^7.4.2",
    "stylelint-config-standard": "^13.0.0",
    "stylelint-selector-bem-pattern": "^1.0.0",
    "uglify-js": "^2.7.2",
    "watch": "^0.19.2",
    "watchify": "^3.7.0",
    "wiredep-cli": "^0.1.0"
  },
  "dependencies": {}
}
